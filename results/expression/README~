# HTSeq and coverageBed are two methods of counting genes.
# For HTSeq:
samtools view $BAM | 
	  sort -s -k 1,1 >$READ_SORTED
usr/local/bin/htseq-count \                                                                                                
    --stranded=no $READ_SORTED $GFF \
    >$HTSEQ_PREFIX.txt 2>$HTSEQ_PREFIX.err 

